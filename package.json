{
  "name": "simple-crud-api",
  "version": "1.0.0",
  "description": "[Task](https://github.com/AlreadyBored/nodejs-assignments/blob/main/assignments/crud-api/assignment.md)",
  "homepage": "https://github.com/Friday-13/simple-crud-api#readme",
  "bugs": {
    "url": "https://github.com/Friday-13/simple-crud-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Friday-13/simple-crud-api.git"
  },
  "license": "ISC",
  "author": "",
  "main": "src/index.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepare": "npm run format; npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node-dev --respawn --transpile-only' src/index.ts",
    "start:prod": "webpack --mode=production && node dist/bundle.js",
    "start:multi": "nodemon --watch 'src/**/*.ts' --exec 'ts-node-dev --respawn --transpile-only' src/index.ts -- --multi",
    "start:prod:multi": "webpack --mode=production && node dist/bundle.js -- --multi"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/jest": "^29.5.14",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  }
}
